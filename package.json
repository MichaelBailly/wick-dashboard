{
	"name": "wick-dashboard",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.0",
		"@smui-extra/accordion": "^6.1.1",
		"@smui/button": "^6.1.0",
		"@smui/checkbox": "^6.1.4",
		"@smui/data-table": "^6.1.0",
		"@smui/drawer": "^6.1.0",
		"@smui/form-field": "^6.1.0",
		"@smui/icon-button": "^7.0.0-beta.0",
		"@smui/linear-progress": "^6.1.4",
		"@smui/list": "^6.1.0",
		"@smui/paper": "^6.1.0",
		"@smui/select": "^6.2.0",
		"@smui/switch": "^6.1.0",
		"@smui/tab": "^6.1.0",
		"@smui/tab-bar": "^6.1.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-node": "^1.0.0-next.92",
		"@sveltejs/kit": "^1.0.1",
		"@types/material__base": "^0.35.2",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^6.1.3",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^5.0.0",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/roboto": "^4.5.8",
		"@fontsource/roboto-mono": "^4.5.8",
		"chart.js": "^3.9.1",
		"chartjs-adapter-date-fns": "^2.0.0",
		"chartjs-plugin-zoom": "^1.2.1",
		"date-fns": "^2.29.3",
		"material-icons": "^1.12.0",
		"mongodb": "^4.9.1"
	}
}
